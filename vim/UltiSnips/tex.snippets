snippet subcaption "set subcaption" b
\\begin{figure}[tb]
	\\centering
	\\subcaptionbox{${2:caption_name}}{
		\\includegraphics[width=${4:\\columnwidth}]{${5:img/path}}
	}
	\\caption{${6:caption_name}}
	% \\ecaption{$8}
	\\label{fig:$7}
\\end{figure}
endsnippet

snippet subfigure_table "set subfigure using table" b
\\begin{table*}[${1:t}]
	\\centering
	\\caption{$2}
	% \\ecaption{$7}
	\\subfigure[\\label{tab:$3}tabname]{
		\\begin{tabular}{$4}\\hline
			$5
		\\end{tabular}
	}
	\\label{tab:$6}
\\end{table*}
endsnippet

snippet enumerate "set enumerate" b
\\begin{enumerate}
	\\item $1
\\end{enumerate}
endsnippet

snippet itemize "set itemize" b
\\begin{itemize}
	\\item $1
\\end{itemize}
endsnippet

snippet description "set description" b
\\begin{description}
	\\item[$1] $2
\\end{description}
endsnippet

snippet equation "set equation" b
\\begin{equation}
	\\label{$1}
	$2
\\end{equation}
endsnippet

snippet eqnarray "set eqnarray" b
\\begin{eqnarray}
	\\label{$1}
	$2 &= \\nonumber
\\end{eqnarray}
endsnippet

snippet aligned "set aligned" b
\\begin{aligned}
	$1
\\end{aligned}
endsnippet

snippet algorithm "set algorithm" b
\\begin{algorithm}
	\\caption{$1}
	\\label{alg:$2}
	\\begin{algorithmic}[1]
		\\Require{${3:Input}}
		\\Ensure{${4:Output}}
		$5
	\\end{algorithmic}
\\end{algorithm}
endsnippet

snippet footnote "set footnote with url" i
\\footnote{\url{$1}}
endsnippet
