snippet section "set section" b
\\section{${1:title}}
$2
endsnippet

snippet subsection "set subsection" b
\\subsection{${1:title}}
$2
endsnippet

snippet begin "set begin" b
\\begin{$1}
	$2
\\end{$1}
endsnippet

snippet figure "set fugure" b
\\begin{figure}[${1:tb}]
	\\centering
	\\includegraphics[width=${2:\\columnwidth}]{${3:path}}
	\\caption{$4}
	% \\ecaption{$6}
	\\label{fig:$5}
\\end{figure}
endsnippet

snippet table "set table" b
\\begin{table}[${1:t}]
	\\centering
	\\caption{$2}
	% \\ecaption{$6}
	\\begin{tabular}{$3}\\hline
		$5
	\\end{tabular}
	\\label{tab:$4}
\\end{table}
endsnippet

snippet bibliography "set bibliography" b
\\bibliographystyle{junsrt}
\\bibliography{bibliography.bib}
endsnippet

